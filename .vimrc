" syntax highlighting
syntax on
colorscheme molokai
set t_Co=256

" activate plugins
filetype plugin on

" tab support
set tabstop=4
set shiftwidth=4
set softtabstop=4
set smarttab
set expandtab

" show number lines
set number

" copying and pasting on system clipboard
set clipboard=unnamed
set clipboard=unnamedplus
set paste
set go+=a
